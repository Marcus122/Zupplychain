<tr data-storage-group="<%=row.storageGroup%>" class="active" data-tr="Expand" data-tr-closed="Collapse">
	<td data-th="Storage Name" class="<%if(row.regType === 'advanced'){%>loom-ignore<%}%> text input-field no-success-validation-style">
		<input type="hidden" value="<%=row.id%>" name="_id"/>
		<div class="tool-tip hint--bottom" data-hint="In ‘quick registration’, you can define your warehouse as one type of pallet that covers all/most of your storage. In ‘advanced registration’, you can add multiple pallet types for which you can define different width/height/weight specifications as well as price differently."><input type="text" data-bind-value="name" data-bind-value-condition="name" required  value="<%=row.name%>" name="name" data-bind-placeholder="placeholder" placeholder="<%=row.placeholder%>" <%if(row.regType === 'quick'){%>disabled<%}%>/></div>
	</td>
	<td data-th="Pallet Type" class="<%if(row.regType === 'advanced'){%>loom-ignore<%}%> select input-field no-success-validation-style">
		<div class="tool-tip hint--top" data-hint="Choose one of the options available. In ‘advance registration’, you can specify the same pallet type with more than one pallet footprint (e.g. 0.8m wide euros, 600 spaces; and 1,2m wide, 400 spaces)."><select name="palletType" required>
        			<option value=""/>
        <% for (i in palletTypes) { %>
            <option data-backend-value="<%=config.palletTypesToWidth[palletTypes[i]]%>" value="<%= i %>" <%= row.palletType == i ? "selected='selected'" : "" %> >
            <%= palletTypes[i] %>
            </option>
            <%
            } //end for palletTypes
            %>
		</select></div>
	</td>
    <td data-th="Storage Temp" class="<%if(row.regType === 'advanced'){%>loom-ignore<%}%> select input-field no-success-validation-style">
		<div class="tool-tip hint--top" data-hint="In ‘quick registration’ only ambient (room temperature) storage can be specified. In ‘advanced registration’, you can also specify bonded.">
        <%if(row.regType === 'quick'){%>
            <input name="temp" type="text" disabled data-backend-value="0" value="<%=temperatures[0]%>"/>
        <%}else{%>
            <select name="temp" required>
                <option value=""/>
                <% var temps = temperatures;
                for (var i in temps) {%>
                <option value="<%= i %>" <%= row.temp == i ? " selected='selected'" : "" %> ><%= temps[i] %></option>
                <%
                } //end for temperatures
                %>
            </select></div>
        <%}%>
	</td>
	<td data-th="Max Weight" class="<%if(row.regType === 'advanced'){%>loom-ignore<%}%> value input-field no-success-validation-style">
		<div class="tool-tip hint--bottom" data-hint="Specify the maximum weight possible for this pallet type."><input name="maxWeight" type="number" value="<%=row.maxWeight%>" placeholder="0" required="required"/></div>
	</td>
	<td data-th="Max Height" class="<%if(row.regType === 'advanced'){%>loom-ignore<%}%> value input-field no-success-validation-style">
		<div class="tool-tip hint--bottom" data-hint="Specify the maximum height possible for this pallet type."><input data-loom-type="decimal" data-loom-decimal-places="2" name="maxHeight" required="required" type="number" value="<%=Number(row.maxHeight).toFixed(1)%>" placeholder="0.0" step="0.1"/></div>
	</td>
	<td data-th="Pallet Spaces" class="<%if(row.regType === 'advanced'){%>loom-ignore<%}%> value input-field no-success-validation-style">
		<div class="tool-tip hint--bottom" data-hint="This is the total number of pallet spaces you have of this pallet type in your warehouse. You can specify how many of these you want to make available on Zupplychain in the next registration step."><input required name="palletSpaces"  type="number" value="<%=row.palletSpaces%>"/></div>
	</td>
    <td data-th="add-additional-pallet-width" class="<%if(row.regType != 'advanced'){%>hidden<%}%> action">
        <button type="button" class="add-button"><span>Add</span></button>
    </td>
    <td data-th="Remove" class="<%if(row.regType != 'advanced'){%>hidden<%}%> action">
        <% if( warehouse.storage && warehouse.storage.length===1){%>
            <button type="button" href="#" class="trash-button hidden"><span>Delete</span></button>
        <%}else{%>
            <button type="button" href="#" class="trash-button"><span>Delete</span></button>
        <%}%>
    </td>
</tr>
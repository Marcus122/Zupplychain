                <script id="result-table-row" type="text/template">
                <tr data-bind-Id="_id">
                                    <td>@num@.</td>
                                    <td>@name@</td>
                                    <td>@city@</td>
                                    <td data-field="distance" data-type="number">@distanceFromSearch@
                                        <!--<span class="ion-thermometer cold"><em class="sr-only">Chilled</em></span>
                                        <span class="ion-thermometer hot"><em class="sr-only">Ambient</em></span>
                                        <span class="ion-ios-snowy cold"><em class="sr-only">Frozen</em></span>-->

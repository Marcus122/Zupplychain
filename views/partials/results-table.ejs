                                    <td data-rating="5" data-field="rating">
                                        <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                        <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                        <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                        <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                        <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                    </td>
                                    <td data-field="price" data-type="number" class="with-pound-sign">@storageMatch.currentPricing.price@</td>
                                    <td class="with-pound-sign">@storageMatch.currentPricing.charge@</td>
                                    <td><a  data-bind-href="href" class=" button action mini view-warehouse-link <%= user.email ? " " : "register-before-click " %>">View</a></td>
                                </tr>

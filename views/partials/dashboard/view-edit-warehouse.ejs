<div class="row" data-view-edit-capture-zone="true">
	<div class="view-warehouse-utility-buttons">
		<div class="view-edit-buttons">
			<a class="toggle-view-edit down" data-mode="view">View</a>
			<a class="toggle-view-edit" data-mode="edit">Edit</a>
		</div>
		<div data-function="save-buttons header" class="save-button header hidden" data-view-edit-capture-zone="true">
			<button type="button" class="action save">Save</button>
		</div>
	</div>
	<section class="tabs">
		<ul class="tabs">
			<li class="four columns active checked" data-trigger-type="click" data-trigger-id="#registration .save" data-validate-form="registration">
                <%if((warehouse && warehouse.storage && warehouse.storage[0]) || (typeof warehouseNum !== 'undefined' && parseInt(warehouseNum) > 0 && !user.company.warehouses[0].equals(warehouse.id))){%>
                    <a href="#step-1"><span>Step 1</span> - Your warehouse details</a>
                <%}else{%>
                    <p data-href="#step-2"><span>Step 1</span> - Your warehouse details</p>
                <%}%>
            </li>
            <li class="four columns checked" data-trigger-type="click" data-trigger-id="#define-space .save" data-validate-form="define-space">
                <%if((warehouse && warehouse.storage && warehouse.storage[0]) || (typeof warehouseNum !== 'undefined' && parseInt(warehouseNum) > 0 && !user.company.warehouses[0].equals(warehouse.id))){%>
                    <a href="#step-2"><span>Step 2</span> - Your storage details</a>
                <%}else{%>
                    <p data-href="#step-2"><span>Step 2</span> - Your storage details</p>
                <%}%>
            </li>
			<li class="four columns checked" data-trigger-type="click" data-trigger-id="#save-and-finish">
                <%if(warehouse && warehouse.storage && warehouse.storage[0]){%>
                    <a href="#step-3"><span>Step 3</span> - Your Pricing and Availablility</a>
                <%}else{%>
                    <p data-href="#step-3"><span>Step 3</span> - Your Pricing and Availablility</p>
                <%}%>
            </li>
		</ul>
	</section>
	<section class="tab-content view-edit-warehouse">
		<div id="step-1" class="tab active">
			<% include registration-1%>
		</div>
		<div id="step-2" class="hidden tab">
			<% include registration-2%>
		</div>
		<div id="step-3" class="hidden tab">
			<% include registration-3%>
		</div> 
	</section>
</div>
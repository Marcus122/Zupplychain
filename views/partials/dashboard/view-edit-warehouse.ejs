<div class="row" data-view-edit-capture-zone="true">
	<div class="view-warehouse-utility-buttons">
		<div class="view-edit-buttons">
			<a class="toggle-view-edit down" data-mode="view">View</a>
			<a class="toggle-view-edit" data-mode="edit">Edit</a>
		</div>
		<div data-function="save-buttons header" class="save-button header" data-view-edit-capture-zone="true">
			<button type="button" class="action save">Save</button>
		</div>
	</div>
	<section class="tabs">
		<ul class="tabs">
			<li class="four columns active checked" data-trigger-type="click" data-trigger-id="#registration .save" data-validate-form="registration"><a href="#step-1"><span>Step 1</span> - Warehouse details</a></li>
			<li class="four columns checked" data-trigger-type="click" data-trigger-id="#define-space .save" data-validate-form="define-space"><a href="#step-2"><span>Step 2</span> - Define your storage</a></li>
			<li class="four columns checked" data-trigger-type="click" data-trigger-id="#save-and-finish"><a href="#step-3"><span>Step 3</span> - Price and availability</a></li>
		</ul>
	</section>
	<section class="tab-content view-edit-warehouse">
		<div id="step-1" class="tab active">
			<% include registration-1%>
		</div>
		<div id="step-2" class="hidden tab">
			<% include registration-2%>
		</div>
		<div id="step-3" class="hidden tab">
			<% include registration-3%>
		</div> 
	</section>
</div>
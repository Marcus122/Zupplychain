<table class="simple dashboard-table" data-loom-num-per-page="5" data-type="warehouses-table">
<thead>
	<tr>
		<th>Warehouse Name</th>
		<th>Address</th>
		<th>View/Edit Warehouse</th>
        <th>View Warehouse Profile</th>
	</tr>
</thead>
<tbody>
	<%for (var i=0; i<warehouses.length; i++){%>
		<tr data-id="<%=warehouses[i].id%>">
			<td data-field="name"><%=warehouses[i].name%></td>
			<td data-field="address"><%=warehouses[i].city%>, <%=warehouses[i].postcode%></td>
			<td data-field="view-edit-warehouse"><a data-action="view-edit-warehouse" class="action button" href="/view-edit-warehouse/<%=warehouses[i]._id%>">View</a></td>
		    <td data-field="view-warehouse-profile"><a href="/warehouse-profile/<%=warehouses[i]._id%>"  target="_blank" class="action button" data-go-to-page="true">View</a></td>
        </tr>
	<%}%>
</tbody>
</table>
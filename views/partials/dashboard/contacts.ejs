<div data-view="contacts" class="dashboard-container columns nine hidden">
	<div class="secondary content-box">
		<div class="header">
			<h3>contacts</h3>
			<a href="/contacts-explanation" class="button action">View Contacts Explanation</a>
		</div>
		<div class="main">
			<section class="tabs">
				<ul class="tab-links tabs two-tabs">
					<%if(authorisations.contacts.masterContacts === true){%>
						<li class="active"><a href="#master-contact">Company Master Contacts</a></li>
						<li><a href="#warehouse-contacts">Warehouse Contacts</a></li>
					<%}else{%>
						<li class="active"><a href="#warehouse-contacts">Warehouse Contacts</a></li>
					<%}%>
				</ul>
			</section>
			<div class="tab-content">
				<%if(authorisations.contacts.masterContacts === true){%>
					<% include multi-used/master-contacts%>
				<%}%>
				<% include ../../partials/dashboard/warehouse-dropdown-list%>
			</div>
		</div>
	</div>
	<%if(warehouses.length === 1){%>
		<div data-one-warehouse="true" class="hidden">
			<% include ../../partials/dashboard/warehouse-specific-contacts%>
		</div>
	<%}%>
</div>
<script type="text/template" id="master-contact-row">
	<% include ../../partials/dashboard/master-contact-row%>
</script>
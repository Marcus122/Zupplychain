<tr>
	<td data-th="From" class="input-field no-success-validation-style">
		<input data-bind-max="to" required type="date" name="from" class="datepicker" data-bind-value="from" data-loom-add-controls="true"/>
	</td>
	<td data-th="To" class="input-field no-success-validation-style">
		<input data-bind-min="from" required data-bind-value="to" type="date" name="to" class="datepicker" data-loom-add-controls="true"/>
	</td>
	<td data-th="Price per pallet per week" class="input-field no-success-validation-style">
		<input data-bind-value="price" name="price" type="number"/>
	</td>
	<td data-th="Handling charge per pallet (In and Out)" class="input-field no-success-validation-style">
		<input data-bind-value="charge" name="charge" type="number"/>
	</td>
	<td data-th="Cost to reserve" class="input-field no-success-validation-style">
		<input data-bind-value="reserve" name="reserve" type="number"/>
	</td>
	<td data-th="Remove" class="input-field no-success-validation-style remove-col">
		<button type="button" class="trash-button"><span>Delete</span></button>
	</td>
</tr>
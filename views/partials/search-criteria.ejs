<section class="side-header quote-form-section"  id="search-area"><!-- a section with the header off to one side -->
	<div class="header">
		<h2>Get an instant quote</h2>
		<span>* required field</span>
	</div>
	<%if (session.whSC != undefined){%>
		<input type="hidden" name="search-cached"/>
		<div class="main">
			<form class="storage-search loom-form" action="/search" id="search-form" novalidate>
				<div class="input-field">
					<label for="pallet-type">Pallet Type*</label>
					<select name="pallet-type" id="pallet-type" required="required">
						<%var palletTypes = ["Blue", "Euro", "Standard", "Anno", "Ant", "Other"];
						for (var i=0; i<palletTypes.length; i++){%>
							<option value="<%=palletTypes[i]%>" <%if (session.whSC.sc[0].palletType === palletTypes[i]){%>
																	selected
																<%}%>><%=palletTypes[i]%></option>
						<%}%>
					</select>
				</div>
				<div class="input-field number">
					<label for="quantity">Qty*</label>
					<input type="number" name="quantity" min="1" max="2000" required="required" value="<%=session.whSC.sc[0].searchQty%>"/>
				</div>
				<div class="input-field">
					<label for="postcode">Target Postcode*</label>
					<input type="text" name="postcode" required="required" data-loom-validators="UKPostCode" value="<%=session.whSC.sc[0].postcode%>"/>
				</div>
				<div class="input-field">
					<label for="max-distance">Max Distance*</label>
					<input type="text" name="max-distance" required="required" value="<%=session.whSC.sc[0].maxDistance%>"/>
				</div>
				<div class="input-field">
					<label for="description">Product Description*</label>
					<input type="text" name="description" minlength="0" maxlength="100" placeholder="" required="required" value="<%=session.whSC.sc[0].description%>"/>
				</div><br/>
				<div class="input-field">
					<label for="start-date">Start Date</label>
					<input type="date" name="start-date" value="<%=session.whSC.sc[0].startDate%>"/>
				</div>
				<div class="input-field">
					<label for="end-date">End Date</label>
					<input type="date" name="end-date" value="<%=session.whSC.sc[0].endDate%>"/>
				</div>
				<div class="input-field number">
					<label for="height">Height (m)</label>
					<input type="number" name="height" min="0" max="100" value="<%=session.whSC.sc[0].height%>"/>
				</div>
				<div class="input-field number">
					<label for="weight">Weight (kg)</label>
					<input type="number" name="weight" min="0" max="2000" value="<%=session.whSC.sc[0].weight%>"/>
				</div>
				
				<div class="input-field">
					<label for="temperature">Temp</label>
					<select name="temperature">
						<%var temps = ["Ambient", "Chilled", "Frozen"];
						for (var i=0; i<temps.length; i++){%>
								<option value="<%=temps[i]%>" <%if (session.whSC.sc[0].temp === temps[i]){%>
																	selected
															   <%}%>><%=temps[i]%></option>
						<%}%>
					</select>
					
				</div>
				<div class="input-field last">
					<button type="submit" class="action forward">Search</button>
				</div>
			</form>
		</div>
	<%}else{%>
		<div class="main">
			<form class="storage-search loom-form" action="/search" id="search-form" novalidate>
				<div class="input-field">
					<label for="pallet-type">Pallet Type*</label>
					<select name="pallet-type" id="pallet-type" required="required">
						<option value="Blue">Blue</option>
						<option value="Euro">Euro</option>
						<option value="Standard">Standard</option>
						<option value="Anno">Anno</option>
						<option value="Ant">Ant</option>
						<option value="Other">Other</option>
					</select>
				</div>
				<div class="input-field number">
					<label for="quantity">Qty*</label>
					<input type="number" name="quantity" min="1" max="2000" required="required" value="10"/>
				</div>
				<div class="input-field">
					<label for="postcode">Target Postcode*</label>
					<input type="text" name="postcode" required="required" data-loom-validators="UKPostCode"/>
				</div>
				<div class="input-field">
					<label for="max-distance">Max Distance*</label>
					<input type="text" name="max-distance" required="required" value="100"/>
				</div>
				<div class="input-field">
					<label for="description">Product Description*</label>
					<input type="text" name="description" minlength="0" maxlength="100" placeholder="" required="required"/>
				</div><br/>
				<div class="input-field">
					<label for="start-date">Start Date</label>
					<input type="date" name="start-date" value="2014-01-01"/>
				</div>
				<div class="input-field">
					<label for="end-date">End Date</label>
					<input type="date" name="end-date" value="2016-01-01"/>
				</div>
				<div class="input-field number">
					<label for="height">Height (m)</label>
					<input type="number" name="height" min="0" max="100" value="100"/>
				</div>
				<div class="input-field number">
					<label for="weight">Weight (kg)</label>
					<input type="number" name="weight" min="0" max="2000" value="100"/>
				</div>
				
				<div class="input-field">
					<label for="temperature">Temp</label>
					<select name="temperature">
						<option value="Ambient">Ambient</option>
						<option value="Chilled">Chilled</option>
						<option value="Frozen">Frozen</option>
					</select>
					
				</div>
				<div class="input-field last">
					<button type="submit" class="action forward">Search</button>
				</div>
			</form>
		</div>
	<%}%>
</section>
                        <h2><em>1. Map</em> Results<small>Click to view details</small></h2>
                    </div>
                    <div class="main">
                        <div id="map-container">Map loading...</div>
                        <div class="search-result-info-box fixed" id="search-results-info">
                      
                            <h4><em class="js-name">Warehouse 2</em><span class="selected ion-checkmark js-is-on-quote"></span>
                            </h4>
                            <p class="address js-address">123 Bury New Road, Bur, BL1 1HG</p>
                            <hr/>
                            <img class="js-image" src="/images/warehouse-thumb.jpg" alt="an external view of a warehouse " />
                            <div class="info">
                                <button class="simple mini delete">Remove from quote</button>
                                
                                <div class="ratings js-rating">
                                    <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                    <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                    <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                    <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                    <span class="ion-android-star rating"><em class="sr-only">*</em></span>
                                </div>
                                <div class="temperatures js-temperatures">
                                    <span class="ion-thermometer cold icon-box"></span>
                                    <span class="ion-thermometer hot icon-box"></span>
                                    <span class="ion-ios-snowy cold icon-box"></span>
                                </div>
                            </div>
                            <hr/>
                            <table class="no-style">
                                <thead>
                                    <tr>
                                        <th>pallet type</th>
                                        <th>Size</th>
                                        <th>Available</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>B</td>
                                        <td>2m x 2m</td>
                                        <td>400</td>
                                    </tr>
                                </tbody>
                            </table>
                            <div class="controls">
                                <button class="simple prev mini" id="map-prev-result">PREV</button>
                                <button class="simple next mini" id="map-next-result">NEXT</button>
                            </div>
                            
                            
                        </div>
                    </div>
                    <div class="footer key">
                        <em>Key</em>
                        <dl>
                            <dt><span class="ion-thermometer cold icon-box"></span></dt>
                            <dd>Ambient</dd>
                            <dt><span class="ion-thermometer hot icon-box"></span></dt>
                            <dd>Chilled</dd>
                            <dt><span class="ion-ios-snowy cold icon-box"></span></dt>
                            <dd>Frozen</dd>
                        </dl>
                    </div>
                </div>
                
                
                
                <div class="content-box side-header">
                    <div class="header">
                        <h2><em>2. Refine</em> your search</h2>
                        <button class="action add">Add pallet</button>
                    </div>
                    <div class="main">
                    <form class="storage-search">
                        <table class="simple responsive-table " >
                            <thead>
                                <tr>
                                    <th><span class="ion-trash-a"></span></th>
                                    <th>Pallet type</th>
                                    <th>Qty</th>
                                    <th>Postcode</th>
                                    <th>Max Distance</th>
                                    <th>Start Date</th>
                                    <th>End Date</th>
                                    <th>Height <small>(m)</small></th>
                                    <th>Weight <small>(kg)</small></th>
                                    <th>Value <small>£</small></th>
                                    <th>Type</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td data-th="Delete"><button class="trash-button"><span class="sr-only">delete</span></button></td>
                                    <td data-th="Pallet type"><select><option>Standard Blue</option><option>Standard Red</option></select></td>
                                    <td data-th="Qty"><input type="text" /></td>
									<%if (postcode != undefined){%>
										<td data-th="Postcode"><input type="text" value=<%=postcode%> /></td>
									<%}else{%>
										<td data-th="Postcode"><input type="text" value="BL9 5BN" /></td>
									<%}%>
                                    <td data-th="Max Distance"><input type="text" /></td>
                                                                     <td data-th="Start Date"><input type="text" /></td>
                                    <td data-th="End Date"><input type="text" /></td>
                                    <td data-th="Height"><input type="text" /></td>
                                    <td  data-th="Weight"><input type="text" /></td>
                                    <td  data-th="Value"><input type="text" /></td>
                                    <td  data-th="Type">
                                        <select>
                                            <option></option>
                                            <option></option>
                                        </select>
                                    </td>
                                    
                                </tr>
                            </tbody>
                        </table>
                        </form>
                        
                        
                        
                        
                        
                    </div>
                </div>
                
               
